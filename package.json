{
  "name": "login_bp",
  "version": "1.0.0",
  "description": "MERN stack boiler plate login/signup system",
  "main": "Server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "start:prod": "SET NODE_ENV=production&&nodemon server.js",
